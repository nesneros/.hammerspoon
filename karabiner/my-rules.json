                  "modifiers": {
                     "mandatory": [ ]
                  }
               },
               "to": [
                  {
                     "key_code": "left_alt"
                  }
               ],
               "to_if_alone": [
                  {
                     "key_code": "f16"
                  }
               ],
               "type": "basic"
            }
         ]
      },
      {
         "description": "left_command -> f17",
         "manipulators": [
            {
               "from": {
                  "key_code": "left_command",
                  "modifiers": {
                     "mandatory": [ ]
                  }
               },
               "to": [
                  {
                     "key_code": "left_command"
                  }
               ],
               "to_if_alone": [
                  {
                     "key_code": "f17"
                  }
               ],
               "type": "basic"
            }
         ]
      },
      {
         "description": "right_alt -> f19",
         "manipulators": [
            {
               "from": {
                  "key_code": "right_alt",
                  "modifiers": {
                     "mandatory": [ ]
                  }
               },
               "to": [
